from rest_framework import serializers
from .models import User,Photo, Battle_Log, Like_table

class UserSerializer(serializers.ModelSerializer):
  class Meta:
    model = User

class PhotoSerializer(serializers.ModelSerializer):
  class Meta:
    model = Photo

class Battle_LogSerializer(serializers.ModelSerializer):
  class Meta:
    model = Battle_Log

class Like_tableSerializer(serializers.ModelSerializer):
  class Meta:
    model = Like_table




